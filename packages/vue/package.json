{
  "name": "@touchspin/vue",
  "version": "5.1.0",
  "description": "Vue adapter for TouchSpin with per-renderer subpaths",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./vanilla": {
      "types": "./dist/vanilla.d.ts",
      "import": "./dist/vanilla.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "vue-tsc && vite build",
    "build:test": "vue-tsc && vite build",
    "build:watch": "vue-tsc && vite build --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "@touchspin/renderer-vanilla": "5.0.1",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@touchspin/renderer-vanilla": {
      "optional": true
    }
  },
  "dependencies": {
    "@touchspin/core": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/vue": "^8.0.0",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.5.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.11",
    "vitest": "^3.2.4",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "touchspin",
    "vue",
    "spinner",
    "number-input"
  ],
  "author": "TouchSpin Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/istvan-ujjmeszaros/touchspin-vue.git"
  },
  "bugs": {
    "url": "https://github.com/istvan-ujjmeszaros/touchspin-vue/issues"
  },
  "homepage": "https://github.com/istvan-ujjmeszaros/touchspin-vue#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/istvan-ujjmeszaros"
  }
}
